{
  "name": "@geut/chaas",
  "version": "1.0.0",
  "description": "A GitHub app for managing changelog-related activities",
  "author": "GEUT <contact@geutstudio.com> (geutstudio.com)",
  "license": "ISC",
  "repository": "https://github.com/geut/chaas.git",
  "homepage": "https://github.com/geut/chaas",
  "bugs": "https://github.com/geut/chaas/issues",
  "keywords": [
    "changelog",
    "tooling",
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "version": "chan release ${npm_package_version} && git add ."
  },
  "dependencies": {
    "probot": "^9.2.5",
    "probot-serverless-now": "^2.0.0"
  },
  "devDependencies": {
    "@geut/chan": "^1.3.0",
    "jest": "^24.7.1",
    "nock": "^10.0.6",
    "nodemon": "^1.18.11",
    "smee-client": "^1.1.0",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
